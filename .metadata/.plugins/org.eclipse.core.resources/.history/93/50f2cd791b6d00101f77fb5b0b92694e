package oppInheritance.sec02;

class A {
	public A() {
		System.out.println("기본 생성자 A");
	}
}

class B extends A{
	public B() {
		System.out.println("기본 생성자 B");
	}
}

class C extends B {
	public C() {
		System.out.println("기본 생성자 C");
	}
}

public class SuperConstTest {

	public static void main(String[] args) {
		// C클래스 인스턴스 생성
		C c =new C();

	}

}
