package package2;

public class Operators {

	public static void main(String[] args) {
		// 자바 연산자 정리
		short a = 100;
		short b = -100;
		
		//short result = -a;
		
		//증감연산자 ++ -- : 변수의 값 1증가 또는 감소
		//연산식에 증감연산만 있는 경우
		int x = 10;
		X++;
		System.out.println(x);
		++x;
		System.out.println(x);
	}

}
